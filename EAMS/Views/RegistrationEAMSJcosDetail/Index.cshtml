@model IEnumerable<EAMS.View_Models.RegEAMSJcoIndex>

@{
    ViewBag.Title = "List of Registered JCO/ORs";
}
<div class="page-body">
    <div class="card">
        <div class="card-header">
            <h5>@ViewBag.Title</h5>
            <div class="card-header-right">
                <a href="@Url.Action("Create", "RegistrationEAMSJcosDetail")" title="Click here to create a New Vacancy" class="btn btn-success btn-sm"><i class="fa fa-plus"></i></a>
                <i class="icofont icofont-rounded-down"></i>    
            </div>
        </div>
        <div class="card-block">
            <table class="table table-striped table-bordered w-100" id="myTable">
                <thead>
                    <tr>
                        <th>
                            Category
                        </th>
                        <th>
                            Service No
                        </th>
                        <th>
                            Rank
                        </th>
                        <th>
                           Name
                        </th>
                        <th>
                            Father Name
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.DOB)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Gender)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.CorpId)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.TradeId)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Religion)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.AdditionalQualification)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.MedCategory)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.PANNo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.AadharNo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.HomeStates.StateName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.DOR)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.RetirementOrderNo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.CAddress)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.CStates.StateName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.CCitys.CityName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.CPin)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.PAddress)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.PStates.StateName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.PCitys.CityName)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.PPin)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.LandlineNo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.MobileNo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Email)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.RecordOffice)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.ROfilesPath)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Qualifications.QualificationTypeString)
                        </th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.ServiceType)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ServiceNo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Ranks.RankName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FirstName)
                            @Html.DisplayFor(modelItem => item.MiddleName)
                            @Html.DisplayFor(modelItem => item.LastName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FatherName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DOB)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Corps.CorpName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TradeId)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Gender)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Religion)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.AdditionalQualification)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.MedCategory)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.PANNo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.AadharNo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.HomeStates.StateName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DOR)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.RetirementOrderNo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CAddress)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CStates.StateName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CCitys.CityName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CPin)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.PAddress)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.PStates.StateName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.PCitys.CityName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.PPin)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.LandlineNo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.MobileNo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Email)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.RecordOffice)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ROfilesPath)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Qualifications.QualificationTypeString)
                        </td>
                        <td>
                            <a href="@Url.Action("Edit", "RegistrationEAMSJcosDetail", new { id = item.RegistrationID })"><i class="fa fa-edit"></i></a>
                            <button type="button" class="btn btn-transparent red btn-xs" onclick="Delete(@item.RegistrationID, this)"><i class="fa fa-trash"></i></button>
                        </td>
                    </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
@section Scripts {
    <script>
        LoadJDT('myTable');

        function Delete(id, event) {
            var TableId = "myTable";
            var Jsonurl = "/RegistrationEAMSJcosDetail/DeleteOnConfirm";
            DeleteJDTRow(id, event, TableId, Jsonurl)
        }
    </script>
}